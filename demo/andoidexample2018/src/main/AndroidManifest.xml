<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oyz.www.andoidexample2018">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demo.PercentActivity"
            android:label="@string/title_percent" />
        <activity
            android:name=".demo.SnackbarActivity"
            android:label="@string/title_activity_snackbar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".demo.FloatingActionButtonActivity"
            android:label="@string/floating_button" />
        <activity
            android:name=".demo.TabLoyoutActivity"
            android:label="@string/title_activity_tab_loyout" />
        <activity
            android:name=".demo.NavigationViewActivity"
            android:label="@string/title_activity_navigation_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".demo.CActivity"></activity>
    </application>

</manifest>